<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/kchart_config_indicator_ring" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item
        android:top="4dp"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/chart_ma3" />
            <corners android:radius="2dp" />
        </shape>
    </item>


</layer-list>